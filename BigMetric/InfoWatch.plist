<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>BGTaskSchedulerPermittedIdentifiers</key>
	<array>
		<string>com.grantperry.BigMetric.watchkitapp.refresh</string>
	</array>
	<key>CFBundleDisplayName</key>
	<string>BigMetric</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIconName</key>
	<string>AppIcon</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>BigMetric</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(MARKETING_VERSION)</string>
	<key>CFBundleVersion</key>
	<string>$(CURRENT_PROJECT_VERSION)</string>
	<key>NSHealthShareUsageDescription</key>
	<string>BigMetric needs access to your health data to function properly.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>BigMetric writes workout data to HealthKit to track your performance.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>BigMetric uses your location to record routes and improve workout tracking.</string>
	<key>NSMotionUsageDescription</key>
	<string>BigMetric uses motion data to detect workouts and track your activity during exercise.</string>
	<key>WKApplication</key>
	<true/>
	<key>WKBackgroundModes</key>
	<array>
		<string>workout-processing</string>
	</array>
	<key>WKCompanionAppBundleIdentifier</key>
	<string>com.grantperry.BigMetric</string>
	<key>WKRunsIndependentlyOfCompanionApp</key>
	<true/>
	<key>WKWatchOnlyApp</key>
	<true/>
</dict>
</plist>
